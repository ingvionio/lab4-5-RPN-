<Window x:Class="WpfForRPN.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WpfForRPN"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <TextBox x:Name="tbInput" HorizontalAlignment="Left" Margin="202,33,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="145" Height="30" TextChanged="TextBox_TextChanged"/>
        <Label x:Name="lblOutput" Content="" HorizontalAlignment="Left" Height="36" Margin="202,99,0,0" VerticalAlignment="Top" Width="145"/>
        <Button x:Name="btnCalculate" Content="Вычислить" HorizontalAlignment="Left" Height="38" Margin="202,164,0,0" VerticalAlignment="Top" Width="73" Click="btnCalculate_Click"/>
        <TextBox HorizontalAlignment="Left" Height="30" Margin="52,33,0,0" TextWrapping="Wrap" Text="Введите выражение:" VerticalAlignment="Top" Width="123" TextChanged="TextBox_TextChanged_1"/>
        <TextBox HorizontalAlignment="Left" Height="30" Margin="98,102,0,0" TextWrapping="Wrap" Text="Результат:" VerticalAlignment="Top" Width="77" TextChanged="TextBox_TextChanged_1"/>

    </Grid>
</Window>
